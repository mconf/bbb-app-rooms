### Mconf Data API
# URL (with port)
MCONF_DATA_API_URL=http://mconf-api-data-svc.app-api.svc.cluster.local:9000
# Feature flag to control the display of Reports button, page and routes
MCONF_DATA_REPORTS_ENABLED=false

### BBB API timeout configs
# get_all_meetings ajax calls
MCONF_AJAX_TIMEOUT=15000
# 'bigbluebutton-api-ruby' gem calls
MCONF_BBB_API_TIMEOUT=15

# Session cookie key (defaults to '_app_rooms_session_' + app_version)
SESSION_COOKIE_KEY=_app_rooms_session

