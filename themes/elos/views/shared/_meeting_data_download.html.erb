<% artifact_files = Mconf::DataApi.get_meeting_artifacts_files(@app_launch.params['custom_params']['institution_guid'], @meeting[:internalMeetingID])%>
<% if artifact_files['shared_notes'].present? %>
  <%= link_to artifact_files['shared_notes'], target: (browser.safari? ? '_self' : '_blank'), class: "dropdown-item rec-edit" do %>
    <%= icon_download %>
    <%= t('meetings.meeting_data_download.download_notes') %>
  <% end %>
<% else %>
  <%= link_to '#', class: "dropdown-item rec-edit disabled_link" do %>
    <%= t('meetings.meeting_data_download.unavailable_notes') %>
  <% end %>
<% end %>

<% if artifact_files['participants_list'].present? %>
  <%= link_to artifact_files['participants_list'], target: (browser.safari? ? '_self' : '_blank'), class: "dropdown-item rec-edit" do %>
    <%= icon_download %>
    <%= t('meetings.meeting_data_download.download_participants') %>
  <% end %>
<% else %>
  <%= link_to '#', class: "dropdown-item rec-edit disabled_link" do %>
    <%= t('meetings.meeting_data_download.unavailable_participants') %>
  <% end %>
<% end %>

<% if artifact_files['engagement_report'].present? %>
  <%= link_to artifact_files['engagement_report'], target: (browser.safari? ? '_self' : '_blank'), class: "dropdown-item rec-edit" do %>
    <%= icon_learning_dashboard %>
    <%= t('meetings.meeting_data_download.dashboard') %>
  <% end %>
<% else %>
  <%= link_to '#', class: "dropdown-item rec-edit disabled_link" do %>
    <%= t('meetings.meeting_data_download.unavailable_dashboard') %>
  <% end %>
<% end %>
