<% if @artifact_files.present? && @artifact_files['shared_notes'].present? %>
  <%= link_to @artifact_files['shared_notes'], target: (browser.safari? ? '_self' : '_blank'),
  class: "dropdown-item rec-edit", data: { 'tracking-id': 'lti-download-participants-list' } do %>
    <%= icon_download %>
    <%= t('meetings.meeting_data_download.download_notes') %>
  <% end %>
<% else %>
  <%= link_to '#', class: "dropdown-item rec-edit disabled_link" do %>
    <%= t('meetings.meeting_data_download.unavailable_notes') %>
  <% end %>
<% end %>

