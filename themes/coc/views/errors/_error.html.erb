    <% if explanation %>
      <div class="explanation-text">
        <%= explanation %>
      </div>
    <% end %>
    <div class="error-logo-all" id="error<%= code %>-logo"></div>
    <div class="title-text">
      <%= "#{t('default.app.error').upcase} #{code} â€“ #{message}" %>
    </div>
  </div>
</div>
